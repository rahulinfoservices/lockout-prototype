{
  "name": "lockout-prototype",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "npx expo run:android",
    "ios": "npx expo run:ios",
    "lint": "expo lint",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "ios:clean": "npx expo prebuild -p ios --clean",
    "android:clean": "npx expo prebuild -p android --clean",
    "ios:build-simulator": "eas build --platform ios --profile ios-simulator",
    "ios:build-development": "eas build --platform ios --profile development",
    "ios:build-preview": "eas build --platform ios --profile preview",
    "android:build-development": "eas build --platform android --profile development",
    "android:build-preview": "eas build --platform android --profile preview",
    "build:development": "eas build --platform all --profile development",
    "build:preview": "eas build --platform all --profile preview"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@gorhom/bottom-sheet": "^5.2.6",
    "@hookform/resolvers": "^5.2.2",
    "@react-native-community/datetimepicker": "^8.5.0",
    "@react-native-firebase/app": "^23.5.0",
    "@react-native-firebase/auth": "^23.5.0",
    "@react-native-firebase/crashlytics": "^23.5.0",
    "@react-native-firebase/database": "^23.5.0",
    "@react-native-firebase/firestore": "^23.5.0",
    "@react-navigation/native": "^7.1.8",
    "date-fns": "^4.1.0",
    "expo": "~54.0.20",
    "expo-build-properties": "^1.0.9",
    "expo-constants": "~18.0.10",
    "expo-dev-client": "~6.0.16",
    "expo-font": "~14.0.9",
    "expo-image": "^3.0.10",
    "expo-linking": "~8.0.8",
    "expo-notifications": "~0.32.12",
    "expo-router": "~6.0.13",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-system-ui": "~6.0.8",
    "expo-updates": "~29.0.12",
    "expo-web-browser": "^15.0.9",
    "lucide-react-native": "^0.553.0",
    "nativewind": "5.0.0-preview.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.66.0",
    "react-native": "0.81.5",
    "react-native-css": "^3.0.1",
    "react-native-gesture-handler": "^2.29.1",
    "react-native-keyboard-controller": "^1.19.5",
    "react-native-modal-datetime-picker": "^18.0.0",
    "react-native-reanimated": "^4.1.5",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "^4.18.0",
    "react-native-svg": "^15.14.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "^0.6.1",
    "tailwind-variants": "^3.1.1",
    "tailwindcss": "^4.1.17",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@tailwindcss/postcss": "^4.1.16",
    "@types/react": "~19.1.0",
    "eslint": "^9.38.0",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-no-secrets": "^2.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "lefthook": "^2.0.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "react-test-renderer": "19.1.0",
    "typescript": "~5.9.2"
  },
  "private": true,
  "pnpm": {
    "onlyBuiltDependencies": [
      "lefthook",
      "unrs-resolver"
    ],
    "overrides": {
      "lightningcss": "1.30.1"
    }
  }
}
